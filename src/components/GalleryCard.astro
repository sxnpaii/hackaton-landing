---
import { Image } from "astro:assets";
const { ImgData, className } = Astro.props;
import "../styles/components-styles/GalleryCard.scss"
---

<div class={`GalleryCard ${className}`}>
  <Image
    src={ImgData.image}
    alt={ImgData.description}
    width={300}
    height={300}
  />
  <div class="bottom">
    <h6>{ImgData.title}</h6>
    <p>{ImgData.description}</p>
  </div>
</div>

